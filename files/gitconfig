[user]
    name = Stu Zhao
    email = z12y12l12@gmail.com
[color]
    ui = true
[alias]
    co = checkout
    ci = commit -v
    st = status
    br = branch
    up = pull --rebase --autostash
    tree = log --oneline --graph --all --decorate
    di = diff
    ds = diff --cached

    git = "!exec git"
[push]
    default = current
[url "git@github.com:"]
    insteadOf = https://github.com/
[core]
    excludesfile = ~/.gitignore_global
[include]
    path = ~/.gitconfig.local
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    required = true
[fetch]
    prune = true

